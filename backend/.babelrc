{
    "presets": [
      [
        "@babel/env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "env": {
      "production": {
        "plugins": [
          [
            "inline-dotenv",
            {
              "path": ".env"
            }
          ]
        ]
      },
      "development": {
        "plugins": [
          [
            "inline-dotenv",
            {
              "path": ".env"
            }
          ]
        ]
      }
    },
    "plugins": ["@babel/plugin-proposal-class-properties", "@babel/plugin-proposal-object-rest-spread"]
  }
  